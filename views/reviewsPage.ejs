<ul>
        <% reviews.forEach(function(review){ %>
             <li>
                 <p><%= review.user_id.displayName %></p>
                 <img src="<%= review.user_id.imgUrl%>" width="100px" alt="">
                 <p><%=review.rating%></p>
                 <p><%=review.body%></p>
                 <span>Post date: <%=review.created_at%></span>
             </li>
        <% }) %>
     
     </ul>